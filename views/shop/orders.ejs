<%- include('../includes/head.ejs') %> 
<main>
    <% if (orders.length <= 0) { %>
        <h1>No Orders yet!</h1>
    <% } else { %>
        <ul>
            <% orders.forEach(order => { %> 
            <li>
                <h1># <%= order._id %> </h1>
                <ul>
                    <% order.items.forEach(products => { %> 
                        <li>
                            <%= products.title %> (<%= products.quantity %>)
                        </li>
                    <% }) %> 
                </ul>
            </li>
            <% }) %> 
        </ul>
    <% } %> 
</main>
<%- include('../includes/end.ejs') %> 
